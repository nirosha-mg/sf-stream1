    </formulas>
    <formulas>
        <processMetadataValues>
            <name>originalFormula</name>
            <value>
                <stringValue>OR
(
AND
(
ISCHANGED([Invoice__c].Status__c), 
NOT (ISBLANK([Invoice__c].Course_Registration__r.Course_Run_Id__r.Owner:User.Id)),
NOT (ISBLANK([Invoice__c].Course_Registration__r.Contact__r.Email)),
OR
(
ISPICKVAL([Invoice__c].Status__c, &quot;Unpaid&quot;),
ISPICKVAL([Invoice__c].Status__c, &quot;Partial&quot;),
ISPICKVAL([Invoice__c].Status__c, &quot;Paid&quot;),
ISPICKVAL([Invoice__c].Status__c, &quot;Refunded&quot;)
)
),
AND
(
ISNEW(),
NOT (ISBLANK([Invoice__c].Course_Registration__r.Course_Run_Id__r.Owner:User.Id)),
NOT (ISBLANK([Invoice__c].Course_Registration__r.Contact__r.Email))
) 
)</stringValue>
            </value>
        </processMetadataValues>
        <name>formula_myRule_3</name>
        <dataType>Boolean</dataType>
        <expression>OR
(
AND
(
ISCHANGED({!myVariable_current.Status__c}), 
NOT (ISBLANK({!myVariable_current.Course_Registration__r.Course_Run_Id__r.Owner:User.Id})),
NOT (ISBLANK({!myVariable_current.Course_Registration__r.Contact__r.Email})),
OR
(
ISPICKVAL({!myVariable_current.Status__c}, &quot;Unpaid&quot;),
ISPICKVAL({!myVariable_current.Status__c}, &quot;Partial&quot;),
ISPICKVAL({!myVariable_current.Status__c}, &quot;Paid&quot;),
ISPICKVAL({!myVariable_current.Status__c}, &quot;Refunded&quot;)
)
),
AND
(
ISNEW(),
NOT (ISBLANK({!myVariable_current.Course_Registration__r.Course_Run_Id__r.Owner:User.Id})),
NOT (ISBLANK({!myVariable_current.Course_Registration__r.Contact__r.Email}))
) 
)</expression>
